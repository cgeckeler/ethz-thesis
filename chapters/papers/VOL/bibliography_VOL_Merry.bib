@article{karban_air_2010,
	title = {An {Air} {Transfer} {Experiment} {Confirms} the {Role} of {Volatile} {Cues} in {Communication} between {Plants}.},
	volume = {176},
	issn = {1537-5323},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/20635861},
	doi = {10.1086/655222},
	abstract = {Abstract: Previous studies reported that sagebrush plants near experimentally clipped neighbors experienced less herbivory than did plants near unclipped neighbors. Blocking air flow with plastic bags made this effect undetectable. However, some scientists remained skeptical about the possibility of volatile communication between plants since the existence and identity of a cue that operates in nature have never been demonstrated. We conducted an air transfer experiment that collected air from the headspace of an experimentally clipped donor plant and delivered it to the headspace of an unclipped assay plant. We found that assay plants treated with air from clipped donors were less likely to be damaged by naturally occurring herbivores in a field experiment. This simple air transfer experiment fulfills the most critical of Koch's postulates and provides more definitive evidence for volatile communication between plants. It also provides an inexpensive experimental protocol that can be used to screen plants for interplant communication in the field.},
	number = {3},
	urldate = {2010-08-05},
	journal = {The American naturalist},
	author = {Karban, Richard and Shiojiri, Kaori and Ishizaki, Satomi},
	month = jul,
	year = {2010},
	pmid = {20635861},
	keywords = {herbivory, cue, eavesdropping, volatile, Fagus sylvatica, ChemSocRev, plant, communication, koch, s postulates},
	pages = {381--384},
}

