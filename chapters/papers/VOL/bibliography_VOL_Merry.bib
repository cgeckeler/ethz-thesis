	keywords = {Agricultural Science},
	file = {Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\KVWI7I7E\\8209.html:text/html;Zakir_2011_Inducible defenses in herbivore-plant.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\6G744BS4\\Zakir_2011_Inducible defenses in herbivore-plant.pdf:application/pdf},
