}

@article{aboubakar_souna_volatiles_2019,
	title = {Volatiles from {Maruca} vitrata ({Lepidoptera}, {Crambidae}) host plants influence olfactory responses of the parasitoid {Therophilus} javanus ({Hymenoptera}, {Braconidae}, {Agathidinae})},
	volume = {130},
	issn = {1049-9644},
	url = {https://www.sciencedirect.com/science/article/pii/S1049964418305796},
	doi = {10.1016/j.biocontrol.2018.11.002},
	abstract = {Plants damaged by herbivores are known to release odors attracting parasitoids. However, there is currently no information how leguminous plants damaged by the pod borer Maruca vitrata attract the exotic larval parasitoid Therophilus javanus, which was imported into Benin from the putative area of origin of the pod borer in tropical Asia for assessing its potential as a biological control agent. In this study, we used Y-tube olfactometer bioassays to investigate T. javanus response towards odors emitted by four M. vitrata-damaged host plants: cowpea Vigna unguiculata, the most important cultivated host, and the naturally occurring legumes Lonchocarpus sericeus, Sesbania rostrata and Tephrosia platycarpa. Olfactory attraction of T. javanus was influenced by the species of plant damaged by the pod borer. Moreover, odors released from M. vitrata-infested host plant organs (flowers and pods) were discriminated over non-infested organs in cowpea and T. platycarpa, respectively. These results are discussed in the context of the possible impact of M. vitrata host plants on T. javanus foraging activity and subsequent establishment in natural environments following experimental releases.},
	journal = {Biological Control},
	author = {Aboubakar Souna, Djibril and Bokonon-Ganta, Aimé Hippolyte and Dannon, Elie Ayitondji and Imorou, Nazyhatou and Agui, Benjamin and Cusumano, Antonino and Srinivasan, Ramasamy and Pittendrigh, Barry Robert and Volkoff, Anne-Nathalie and Tamò, Manuele},
	month = mar,
	year = {2019},
	keywords = {Biological control, Olfaction, Host plants, Natural enemy, Attraction, Cowpea},
	pages = {104--109},
}

@article{schutz_biosensor_1996,
	title = {Biosensor for volatiles released by damaged plants},
	volume = {11},
	issn = {0956-5663},
	url = {https://www.sciencedirect.com/science/article/pii/0956566396827382},
	doi = {10.1016/0956-5663(96)82738-2},
	abstract = {One of the most crucial problems in biological or biotechnical plant protection is the timing of starting an application. The proposed biosensor measuring overall degree of plant damage is a useful tool for solving the problem. If plants are damaged, for instance by cuts made artificially or by biting or chewing insects, volatile organic compounds (VOCs) will be released to a higher extent and in a different composition compared with undamaged plants. The biosensor based on an electroantennographic technique uses the sensitivity and selectivity of an insect's antenna to detect the changes in composition of the VOC. This method is comparatively simple, inexpensive and sensitive (e.g. 1 ppbv cis-3-hexen-1-ol, dynamic range: 1 ppbv-100 ppmv) with respect to usual mass-spectrometric methods of VOC trace analysis. Moreover, the response time (10 ms) and the measurement cycle time (3 min) are short and the biosensor yields an overall parameter easier to deal with than complex mass-spectrometric data arrays. The lifetime of one antennal preparation, however, is 4 h and has to be improved. This paper presents results emerging from laboratory experiments with the Colorado potato beetle (Leptinotarsa decemlineata Say).},
	number = {4},
	journal = {Biosensors and Bioelectronics},
	author = {Schütz, S. and Weiβbecker, B. and Hummel, H.E.},
	month = jan,
	year = {1996},
	keywords = {VOC, chemical ecology, biosensor, Colorado potato beetle, electroantennography, insect antenna, plant damage induced signals, trace analysis},
	pages = {427--433},
}

