@article{van_doan_herbivore-induced_2021,
	title = {Herbivore-induced plant volatiles mediate defense regulation in maize leaves but not in maize roots},
	issn = {13653040},
	doi = {10.1111/pce.14052},
	abstract = {Plant leaves that are exposed to herbivore-induced plant volatiles (HIPVs) respond by increasing their defenses, a phenomenon referred to as priming. Whether this phenomenon also occurs in the roots is unknown. Using maize plants, Zea mays, whose leaves respond strongly to leaf HIPVs, we measured the impact of belowground HIPVs, emanating from roots infested by the banded cucumber beetle, Diabrotica balteata, on constitutive and herbivore-induced levels of defense-related gene expression, phytohormones, volatile and non-volatile primary and secondary metabolites, growth and herbivore resistance in roots of neighbouring plants. HIPV exposure did not increase constitutive or induced levels of any of the measured root traits. Furthermore, HIPV exposure did not reduce the performance or survival of D. balteata on maize or its ancestor teosinte. Cross-exposure experiments between HIPVs from roots and leaves revealed that maize roots, in contrast to maize leaves, neither emit nor respond strongly to defense-regulating HIPVs. Together, these results demonstrate that volatile-mediated defense regulation is restricted to the leaves of maize. This finding is in line with the lower diffusibility of volatiles in the soil and the availability of other, potentially more efficient, information conduits below ground.},
	number = {February},
	journal = {Plant Cell and Environment},
	author = {van Doan, Cong and Züst, Tobias and Maurer, Corina and Zhang, Xi and Machado, Ricardo A.R. and Mateo, Pierre and Ye, Meng and Schimmel, Bernardus C.J. and Glauser, Gaétan and Robert, Christelle A.M.},
	year = {2021},
	keywords = {Fagus sylvatica, priming, belowground plant-herbivore interactions, plant-plant interactions, root defenses},
	pages = {1--15},
	file = {PCE_14052_Figure S1.tif:C\:\\Users\\mschuman\\Zotero\\storage\\MUDHA6Y4\\PCE_14052_Figure S1.tif:image/tiff;PCE_14052_FigureS2.jpg:C\:\\Users\\mschuman\\Zotero\\storage\\EN754JYY\\PCE_14052_FigureS2.jpg:image/jpeg;PCE_14052_FigureS3.tiff:C\:\\Users\\mschuman\\Zotero\\storage\\7DTDSK4X\\PCE_14052_FigureS3.tiff:image/tiff;PCE_14052_FigureS4.tiff:C\:\\Users\\mschuman\\Zotero\\storage\\XCJK5RZB\\PCE_14052_FigureS4.tiff:image/tiff;PCE_14052_SourceData.xlsx:C\:\\Users\\mschuman\\Zotero\\storage\\GVWM8SC8\\PCE_14052_SourceData.xlsx:application/vnd.openxmlformats-officedocument.spreadsheetml.sheet;PCE_14052_Table S1.docx:C\:\\Users\\mschuman\\Zotero\\storage\\7BR7MA5J\\PCE_14052_Table S1.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S2.docx:C\:\\Users\\mschuman\\Zotero\\storage\\TTI4MTHW\\PCE_14052_Table S2.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S3.docx:C\:\\Users\\mschuman\\Zotero\\storage\\8XMYIHNN\\PCE_14052_Table S3.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S4.docx:C\:\\Users\\mschuman\\Zotero\\storage\\LDC3R24P\\PCE_14052_Table S4.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S5.docx:C\:\\Users\\mschuman\\Zotero\\storage\\EGTKDPEA\\PCE_14052_Table S5.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S6.docx:C\:\\Users\\mschuman\\Zotero\\storage\\HM4LXC8D\\PCE_14052_Table S6.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;PCE_14052_Table S7.docx:C\:\\Users\\mschuman\\Zotero\\storage\\FJIIZ6DG\\PCE_14052_Table S7.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\7FNF5UF6\\pce.html:text/html;Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\DY8GGRNF\\pce.html:text/html;van Doan_2021_Herbivore-induced plant volatiles.pdf:C\:\\