	editor = {Baluška, František},
	year = {2013},
	doi = {10.1007/978-3-642-36470-9_19},
	keywords = {Herbivore-induced plant volatiles, Priming, Volatile organic compounds, Plant signaling, Trophic interactions, Within-plant communication},
	pages = {393--412},
	file = {Rodriguez-Saona_2013_The Role of Volatiles in Plant–Plant.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\E862S9Z9\\Rodriguez-Saona_2013_The Role of Volatiles in Plant–Plant.pdf:application/pdf},
