	author = {Zito, Pietro and Rosselli, Sergio and Bruno, Maurizio and Maggio, Antonella and Sajeva, Maurizio},
	month = feb,
	year = {2019},
	keywords = {GC-MS, Pollination, Dynamic headspace, Iridaceae, Aromatic compounds, Bumble bees, Honey bees, Hover flies},
	pages = {86--90},
}

@article{paul_preservation_2020,
	title = {Preservation of monoterpenoids in {Oligocene} resin: {Insights} into the evolution of chemical defense mechanism of plants in deep-time},
	volume = {217},
	issn = {0166-5162},
	url = {https://www.sciencedirect.com/science/article/pii/S0166516219308997},
	doi = {10.1016/j.coal.2019.103326},
	abstract = {Monoterpenoid class of compounds is well-known for the characteristic odor and their contribution in various ecological functions such as plant-insect interaction, defense, pollinator attraction etc. They are extremely volatile and labile by nature and therefore their preservation in geological samples is quite uncommon. In the present article we report, exceptionally preserved monoterpenoids in late Oligocene resin from offshore Shimokita, Japan. Compounds viz. fenchol, fenchone, camphor, borneol, isoborneol are detected in appreciable quantity while camphene, isocamphene and p-cymene are found in low amount. Such rare phenomenon suggests that the amber must have experienced some unique diagenetic condition which favored the preservation of these volatile compounds. These monoterpenes have distinct aroma and they actively contribute into the chemical defense mechanisms of extant plant species. Their excellent preservation in late Oligocene amber depicts that such chemical defense strategies had already been developed by plants during early Cenozoic.},
	journal = {International Journal of Coal Geology},
	author = {Paul, Swagata and Gross, Doris and Bechtel, Achim and Dutta, Suryendu},
	month = jan,
	year = {2020},
	keywords = {Chemical defense, Monoterpenoids, Amber, Late Oligocene},
	pages = {103326},
}

@article{poelman_consequences_2008,
	title = {Consequences of variation in plant defense for biodiversity at higher trophic levels},
	volume = {13},
	issn = {1360-1385},
	url = {https://www.sciencedirect.com/science/article/pii/S1360138508002197},
	doi = {10.1016/j.tplants.2008.08.003},
	abstract = {Antagonistic interactions between insect herbivores and plants impose selection on plants to defend themselves against these attackers. Although selection on plant defense traits has typically been studied for pairwise plant–attacker interactions, other community members of plant-based food webs are unavoidably affected by these traits as well. A plant trait might, for example, affect parasitoids and predators feeding on the herbivore. Consequently, defensive plant traits structure the diversity and composition of the complex community associated with the plant, and communities as a whole also feed back to selection on plant traits. Here, we review recent developments in our understanding of how plant defense traits structure insect communities and discuss how molecular mechanisms might drive community-wide effects.},
	number = {10},
	journal = {Trends in Plant Science},
	author = {Poelman, Erik H. and van Loon, Joop J.A. and Dicke, Marcel},
	month = oct,
	year = {2008},
	pages = {534--541},
}

@incollection{pickett_secondary_1999,
