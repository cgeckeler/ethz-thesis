	pmid = {10952311},
	keywords = {Plants, Plant, Fabaceae, Fagus sylvatica, Gene Expression Regulation, Animals, Volatilization, Female, Plant Oils, Terpenes, Plant Proteins, Terpenes: metabolism, Transcriptional Activation, Cyclopentanes, Oxylipins, Salicylates, Plant Proteins: genetics, Medicinal, Fabaceae: physiology, Oils, Volatile, Lipoxygenase, Acetic Acids, Acetic Acids: pharmacology, Cyclopentanes: pharmacology, Volatile: metabolism, Fabaceae: drug effects, Fabaceae: genetics, Plant Oils: metabolism, Lipoxygenase: genetics, Fabaceae: metabolism, Mites, Mites: physiology, Egtazic Acid, Egtazic Acid: analogs \& derivatives, Egtazic Acid: pharmacology, Lipoxygenase: biosynthesis, Salicylates: pharmacology},
