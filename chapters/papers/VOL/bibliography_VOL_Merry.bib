	month = nov,
	year = {2001},
	keywords = {Green leaf volatiles, Lima bean, Pathogenesis-related (PR) genes, Plant–plant interactions, Spider mites ()},
	pages = {1049--1061},
}

@article{nakashima_traumatin-_2013,
	title = {Traumatin- and {Dinortraumatin}-containing {Galactolipids} in {Arabidopsis}: {THEIR} {FORMATION} {IN} {TISSUE}-{DISRUPTED} {LEAVES} {AS} {COUNTERPARTS} {OF} {GREEN} {LEAF} {VOLATILES}*},
