	urldate = {2022-07-02},
	journal = {Biological Control},
	author = {Kaplan, Ian},
	month = feb,
	year = {2012},
	keywords = {Tritrophic interactions, Pest management, HIPVs, Methyl salicylate, Indirect plant defenses, Beneficial insects},
	pages = {77--89},
	file = {Kaplan_2012_Attracting carnivorous arthropods with.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\34HF7RSV\\Kaplan_2012_Attracting carnivorous arthropods with.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\PG2SMJ8T\\S1049964411003057.html:text/html},
