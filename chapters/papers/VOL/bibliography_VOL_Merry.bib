	year = {2019},
	keywords = {biocontrol, Coleoptera, Elateridae, Integrated pest management, Microbial control},
	pages = {100080},
}

@article{pare_cotton_1998,
	title = {Cotton volatiles synthesized and released distal to the site of insect damage},
	volume = {47},
	issn = {0031-9422},
	url = {https://www.sciencedirect.com/science/article/pii/S0031942297004421},
	doi = {10.1016/S0031-9422(97)00442-1},
	abstract = {Cotton plants (Gossypium hirsutum) damaged by herbivorous insects release volatile compounds that act as chemical signals to attract naemies of the herbivore to the damaged plant. Feeding by beet armyworm larvae (Spodoptera exigua) on the lower leaves of cotton plants triggers the systemic release of six readily detectable terpenes and hexenyl acetate, a product of the lipoxygenase pathway. Labelling with 13CO2 established that the terpenes released systemically after herbivore damage are synthesized de novo at or near the site of emission. Hexenyl acetate rapidly incorporated 13C into the acetate but not into the hexenyl moiety within our pulse labelling interval. Thus, activation of the lipoxygenase pathway in undamaged leaves of insect damaged plants is indicated.},
