@article{gouinguene_variability_2001,
	title = {Variability in herbivore-induced odour emissions among maize cultivars and their wild ancestors (teosinte)},
	volume = {11},
	issn = {1423-0445},
	url = {https://doi.org/10.1007/PL00001832},
	doi = {10.1007/PL00001832},
	abstract = {Maize plants respond to caterpillar feeding with the release of relatively large amounts of specific volatiles, which are known to serve as cues for parasitoids to locate their host. Little is known about the genetic variability in such herbivore-induced plant signals and about how the emissions in cultivated plants compare to those of their wild relatives. For this reason we compared the total quantity and the qualitative composition of the odour blend among eleven maize cultivars and five wild Zea (Poaceae) species (teosinte), as well as among the offspring of eight Zea mays mexicana plants from a single population. Young plants were induced to release volatiles by mechanically damaging the leaves and applying oral secretions of Spodoptera littoralis (Lepidoptera: Noctuidae) caterpillars to the wounded sites. Volatiles were collected 7 h after treatment and subsequently analysed by gas chromatography. The total amounts of volatiles released were significantly different among maize cultivars as well as among the teosintes. Moreover, striking differences were found in the composition of the induced odour blends. Caryophyllene, for instance, was released by some, but not all varieties and teosintes, and the ratios among monoterpenes and sesquiterpenes varied considerably. The offspring of different mother plants of the Z. m. mexicana population showed some variation in the total amounts that they released, but the composition of the odour blend was very consistent within the population of this teosinte species. We discuss the ecological significance of these findings in terms of specificity and reliability of induced plant signals for parasitoids.},
	language = {en},
	number = {1},
	urldate = {2022-07-02},
	journal = {CHEMOECOLOGY},
	author = {Gouinguené, Sandrine and Degen, Thomas and Turlings, Ted C. J.},
	month = mar,
	year = {2001},
	keywords = {Maize, Tritrophic interactions, zea mays, tritrophic interactions, Fagus sylvatica, Zea mays, maize, ability, induced plant volatiles, reli-, specificity, Specificity, Induced plant volatiles, Reliability, Teosinte, Key words. Induced plant volatiles – specificity – reliability – tritrophic interactions –Zea mays– maize – teosinte},
	pages = {9--16},
	file = {Gouinguené_2001_Variability in herbivore-induced odour.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\4IRPUYM8\\Gouinguené_2001_Variability in herbivore-induced odour.pdf:application/pdf},
}

@article{baldwin_plant_2010-1,
	title = {Plant volatiles},
