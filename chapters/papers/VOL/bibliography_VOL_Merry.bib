	year = {2016},
	doi = {10.1007/978-3-319-33498-1_13},
	keywords = {Green leaf volatiles, Plant–plant communication, Plant defence, Glycosylation, Volatile terpenoids, Glutathionylation, Oxidation/reduction},
	pages = {305--316},
	file = {Sugimoto_2016_Uptake and Conversion of Volatile.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\YI4MMUMM\\Sugimoto_2016_Uptake and Conversion of Volatile.pdf:application/pdf},
