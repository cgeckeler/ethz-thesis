	keywords = {Ecosystem, Plants, Herbivory, Fagus sylvatica, Animals, Feeding Behavior, Feeding Behavior: physiology, Insects, Insects: physiology, Plants: metabolism, Species Specificity, Volatile Organic Compounds, Insects: classification, Plants: parasitology, Host-Parasite Interactions, Plants: classification, Volatile Organic Compounds: metabolism, Herbivory: classification, Herbivory: physiology},
	pages = {303--310},
