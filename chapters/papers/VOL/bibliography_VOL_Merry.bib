	pages = {107--132},
	file = {Bertrand_2021_Chapter Four - Plant metabolomics to.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\5UZAU86C\\Bertrand_2021_Chapter Four - Plant metabolomics to.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\BCLWKDTS\\S0065229620300781.html:text/html},
