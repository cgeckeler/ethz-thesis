	keywords = {methyl jasmonate, green leaf volatiles, cis-jasmone, volatile organic compounds, ethylene, methyl salicylate, terpenoids, isoprene, plant-plant communications, pyrethrins, within-plant communications},
	pages = {222--226},
	file = {Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\JGTAJFFE\\psb.html:text/html;Ueda_2012_Plant communication.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\NKINCR84\\Ueda_2012_Plant communication.pdf:application/pdf},
}

@article{das_plant_2013,
	title = {Plant volatiles as method of communication},
	volume = {7},
	issn = {1863-5474},
	url = {https://doi.org/10.1007/s11816-012-0236-1},
	doi = {10.1007/s11816-012-0236-1},
	abstract = {Plants emit volatile compounds that can act as a communication method to insects, neighboring plants and pathogens. Plants respond to leaf and root damage by herbivores and pathogens by emitting these compounds. The volatile compounds can deter the herbivores or pathogens directly or indirectly by attracting their natural enemies to kill them. The simultaneous damage of plants by herbivores and pathogens can influence plant defense. The induced plant volatiles can also make neighboring plants ready for defense or induce defense in parts distant from the damaged area of the same plant. Belowground root herbivory can alter the defense response to aboveground leaf herbivory. In addition, most plants normally emit volatile compounds from their flowers that directly attract foraging mutualistic insects for nectar, which in turn perform the very important function of pollination for subsequent reproduction. The volatile compounds emitted from the floral and vegetative parts of plants belong to three main classes of compounds: terpenoids, phenylpropanoids/benzenoids, and C6-aldehydes (green-leaf volatiles). The volatile phytohormones methyl salicylate and methyl jasmonate serve as important signaling molecules for communication purposes, and interact with each other to optimize the plant defense response. Here we discuss and integrate the current knowledge on all types of communication between plants and insects, neighboring plants and pathogens that are mediated through plant volatiles.},
	language = {en},
	number = {1},
	urldate = {2022-05-21},
	journal = {Plant Biotechnology Reports},
	author = {Das, Amitabha and Lee, Sook-Hee and Hyun, Tae Kyung and Kim, Seon-Won and Kim, Jae-Yean},
	month = jan,
	year = {2013},
	keywords = {Plant volatiles, Pollination, Communication, Defense, Terpenoids, Phenylpropanoids, Green-leaf volatiles},
	pages = {9--26},
	file = {Das_2013_Plant volatiles as method of.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\UKRZ72Y2\\Das_2013_Plant volatiles as method of.pdf:application/pdf},
}

