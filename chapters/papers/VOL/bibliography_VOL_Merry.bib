@article{brito_look_2016,
	title = {A look inside odorant-binding proteins in insect chemoreception},
	volume = {95},
	issn = {0022-1910},
	url = {https://www.sciencedirect.com/science/article/pii/S0022191016301081},
	doi = {10.1016/j.jinsphys.2016.09.008},
	abstract = {Detection of chemical signals from the environment through olfaction is an indispensable mechanism for maintaining an insect’s life, evoking critical behavioral responses. Among several proteins involved in the olfactory perception process, the odorant binding protein (OBP) has been shown to be essential for a normally functioning olfactory system. This paper discusses the role of OBPs in insect chemoreception. Here, structural aspects, mechanisms of action and binding affinity of such proteins are reviewed, as well as their promising application as molecular targets for the development of new strategies for insect population management and other technological purposes.},
