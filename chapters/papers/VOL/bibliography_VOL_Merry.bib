	pages = {1058--1066},
	file = {PDF:C\:\\Users\\mschuman\\Zotero\\storage\\THN7FDTD\\2016-Silencing_Nicotiana_attenuata_LHY_and_ZTL_alters_circadian_rhythms_in_flowers.pdf:application/pdf},
