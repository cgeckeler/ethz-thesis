	journal = {Agricultural Sciences in China},
	author = {YANG, Li-mei and Mercke, Per and van Loon, Joop JA and FANG, Zhi-yuan and Dicke, Marcel and Jongsma, Maarten A},
	month = may,
	year = {2008},
	keywords = {expression, inducible promoter, induction, linalool synthase, methyl jasmonate, potato proteins inhibitor},
