@article{pickett_switching_2001,
	title = {Switching on plant genes by external chemical signals},
	volume = {6},
	issn = {1360-1385},
	doi = {10.1016/S1360-1385(01)01899-4},
	abstract = {During the past decade there has been rapidly increasing interest in the role of plant volatiles in insect-plant interactions and the induction of plant defence systems by both pathogens and herbivores. Scientists are striving to link the proximate studies elucidating pathways end genes with the ultimate adaptive studies that attempt to explain their ecological role. However, we still do not know whether plants 'talk' to one another by employing 'phytopheromones'.},
