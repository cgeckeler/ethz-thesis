	keywords = {HIPVs, sexual communication, gas chromatography-mass spectrometry, Heteroptera, infochemical, Miridae, Y-tube olfactometer, mate finding, 5-ethyl-2(5H)-furanone, E, E-α-farnesene, green bean, Z-3 -hexenyl tiglate},
