@article{roda_molecular_2003,
	title = {Molecular technology reveals how the induced direct defenses of plants work},
	volume = {4},
	issn = {1439-1791},
	url = {https://www.sciencedirect.com/science/article/pii/S1439179104700997},
	doi = {10.1078/1439-1791-00130},
	abstract = {Abstract
Plants do not endure herbivore attack passively but respond with a coordinated array of defensive strategies. Many toxic compounds and proteins elicited by herbivore attack are thought to increase the plant's resistance to future attack and thereby function as direct defenses. However, whether these compounds are actually responsible for induced direct defense remains unclear because the available evidence is largely correlative. Recent advances in understanding the genetic basis of resistance traits and their signaling pathways allow researchers to directly manipulate induced direct defenses and we review this growing literature. Also, these studies repeatedly demonstrate that changes in single traits rarely account for the phenomenon of induced defense. Transcriptional analysis performed during induction reveals the complexity of the metabolic reconfiguration in which direct defense traits are embedded. We conclude that genetic alterations of regulatory elements coordinating these complex changes will likely provide the most revealing manipulations and consider the controls necessary for a functional analysis of putative defense traits with transformed plants. Pflanzen erleiden Angriffe durch Herbivore nicht passiv, sondern reagieren mit einer Reihe koordinierter Verteidigungsstrategien. Für viele toxische Verbindungen und auch Proteine, die durch Herbivorenfraß induziert werden, nimmt man an, dass sie die pflanzliche Resistenz gegen weitere Angriffe erhöhen und somit als direkte Verteidigungsmaßnahme fungieren. Ob aber diese Verbindungen tatsächlich verantwortlich sind für eine direkte Abwehr, ist oft unklar, weil es dafür zumeist nur korrelative Anhaltspunkte gibt. Wir geben hier einen Überblick der in letzter Zeit erworbenen Erkenntnisse über die genetischen Grundlagen der pflanzlichen Resistenz und der entsprechenden Signalwege. Diese Informationen erlauben uns eine genetische Manipulation der induzierten direkten Abwehrreaktionen der Pflanzen. Die hier aufgezeigten Studien zeigen wieder einmal, dass Veränderungen einzelner Merkmale kaum als Erklärung für das Phänomen der induzierten Verteidigung dienen können. Transkriptionsanalysen, die während der Induktion in der Pflanze durchgeführt werden, offenbaren die Komplexität der Stoffwechselveränderungen, die bei Angriff durch Herbivore induziert werden. Genetische Veränderungen der regulatorischen Elemente, die diese komplexen Veränderungen koordinieren, bieten vermutlich die beste Möglichkeit der pflanzlichen Manipulation, um so die Grundlagen der Abwehrmechanismen zu verstehen, und die Kontrollen zu berücksichtigen, die für eine Funktionsanalyse der vermuteten pflanzlichen Abwehrmaßnahmen notwendig sind.},
	number = {1},
	journal = {Basic and Applied Ecology},
	author = {Roda, Amy L. and Baldwin, Ian T.},
	month = jan,
	year = {2003},
	keywords = {direct and indirect defenses, plant-herbivore interactions, secondary metabolites, transformation},
	pages = {15--26},
}

@article{carrasco_insect_2015,
	title = {Insect host plant selection in complex environments},
	volume = {8},
