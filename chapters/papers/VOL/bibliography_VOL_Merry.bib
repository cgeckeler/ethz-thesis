@incollection{spiteller_plant_2008,
	address = {Oxford},
	title = {Plant {Defense} {Strategies}},
	isbn = {978-0-08-045405-4},
	url = {https://www.sciencedirect.com/science/article/pii/B9780080454054009046},
	abstract = {Plants are primary food sources for many organisms and therefore require effective defense mechanisms. Some plants produce defense compounds such as toxic alkaloids, terpenoids, phenolic compounds, or saponins which instantly act against herbivore attack (constitutive defense). Also mechanical barriers, sticky secretions, a pungent, bitter taste, or extreme pH may deter the organisms immediately from feeding. Instead, some plants upregulate their defense mechanisms only after provocation (induced defense). A complex recognition and signaling system including jasmonic acid and salicylate ensures an adequate response. Induced defense has the drawback that some time passes from recognition to defense, yet if not attacked all resources can be invested in growth. The production of toxins (phytoalexins) or proteinase inhibitors is dramatically increased. Together with nutrient allocation, the latter contribute to lowering of the feeding success of herbivores. Included in the induced defense is the production of volatiles in order to warn neighboring plants as well as to attract help from predatory insects. Besides, the nectar flow of extrafloral nectaries is induced providing a reward to insects that remove herbivores. As a third general strategy, some plants release highly reactive toxins quickly by enzymatic activation from inactive storage forms such as benzoxazinoids, iridoids, cyanogenic glycosides, or glucosinolates (activated defense).},
	language = {en},
	urldate = {2022-07-03},
	booktitle = {Encyclopedia of {Ecology}},
	publisher = {Academic Press},
	author = {Spiteller, D.},
	editor = {Jørgensen, Sven Erik and Fath, Brian D.},
	month = jan,
	year = {2008},
	doi = {10.1016/B978-008045405-4.00904-6},
	keywords = {Jasmonic acid, Terpenes, Oxylipins, Phenolics, Saponins, Glucosinolates, Lignin, Volatiles, Elicitors, Volicitin, Cyanogenic glycosides, Phytoalexins, Protease inhibitors},
	pages = {2798--2811},
	file = {ScienceDirect Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\RE3N59PG\\B9780080454054009046.html:text/html;Spiteller_2008_Plant Defense Strategies.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\ALMP2JZ3\\Spiteller_2008_Plant Defense Strategies.pdf:application/pdf},
}

@article{jamieson_global_2017,
	series = {Global change biology * {Molecular} physiology},
	title = {Global change effects on plant–insect interactions: the role of phytochemistry},
	volume = {23},
	issn = {2214-5745},
	shorttitle = {Global change effects on plant–insect interactions},
	url = {https://www.sciencedirect.com/science/article/pii/S2214574517300627},
	doi = {10.1016/j.cois.2017.07.009},
	abstract = {Natural and managed ecosystems are undergoing rapid environmental change due to a growing human population and associated increases in industrial and agricultural activity. Global environmental change directly and indirectly impacts insect herbivores and pollinators. In this review, we highlight recent research examining how environmental change factors affect plant chemistry and, in turn, ecological interactions among plants, herbivores, and pollinators. Recent studies reveal the complex nature of understanding global change effects on plant secondary metabolites and plant–insect interactions. Nonetheless, these studies indicate that phytochemistry mediates insect responses to environmental change. Future research on the chemical ecology of plant–insect interactions will provide critical insight into the ecological effects of climate change and other anthropogenic disturbances. We recommend greater attention to investigations examining interactive effects of multiple environmental change factors in addition to chemically mediated plant–pollinator interactions, given limited research in these areas.},
	language = {en},
	urldate = {2022-07-03},
	journal = {Current Opinion in Insect Science},
	author = {Jamieson, Mary A and Burkle, Laura A and Manson, Jessamyn S and Runyon, Justin B and Trowbridge, Amy M and Zientek, Joseph},
	month = oct,
