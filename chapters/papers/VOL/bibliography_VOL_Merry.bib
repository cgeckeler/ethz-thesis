	keywords = {Chemical ecology, Semiochemical, Pheromone, Kairomone, Synomone, Primer, Recruitment, Sex, Aggregation, Alarm, Allelochemical, Allomone, Home range marking, Quorum sensing, Releaser, Social},
	pages = {557--575},
