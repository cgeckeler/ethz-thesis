	editor = {Singh, Indrakant K. and Singh, Archana},
	year = {2021},
	doi = {10.1007/978-981-15-2467-7_7},
	keywords = {Insect olfaction, Plant volatiles, Host selection, Non-host volatiles (NHVs), Odourant receptor (OR), Push-pull strategy},
	pages = {127--156},
	file = {Binyameen_2021_Plant Volatiles and Their Role in.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\UB3E6KDF\\Binyameen_2021_Plant Volatiles and Their Role in.pdf:application/pdf},
