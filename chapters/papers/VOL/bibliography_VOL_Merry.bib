@article{hountondji_herbivore-induced_2005,
	title = {Herbivore-induced plant volatiles trigger sporulation in entomopathogenic fungi: {The} case of {Neozygites} tanajoae infecting the cassava green mite},
	volume = {31},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-21044450426&doi=10.1007%2fs10886-005-4244-2&partnerID=40&md5=6ba07cf0358b348e0427adcddeae0f56},
	doi = {10.1007/s10886-005-4244-2},
	abstract = {A large body of evidence shows that plants release volatile chemicals upon attack by herbivores. These volatiles influence the performance of natural enemies. Nearly all the evidence on the effect of plant volatiles on natural enemies of herbivores concerns predators, parasitoids, and entomophagous nematodes. However, other entomopathogens, such as fungi, have not been studied yet for the way they exploit the chemical information that the plant conveys on the presence of herbivores. We tested the hypothesis that volatiles emanating from cassava plants infested by green mites (Mononychellus tanajoa) trigger sporulation in three isolates of the acaropathogenic fungus Neozygites tanajoae. Tests were conducted under climatic conditions optimal to fungal conidiation, such that the influence of the plant volatiles could only alter the quantity of conidia produced. For two isolates (Altal.brz and Colal.brz), it was found that, compared with clean air, the presence of volatiles from clean, excised leaf discs suppressed conidia production. This suppressive effect disappeared in the presence of herbivore-damaged leaves for the isolate Colal.brz. For the third isolate, no significant effects were observed. Another experiment differing mainly in the amount of volatiles showed that two isolates produced more conidia when exposed to herbivore-damaged leaves compared with clean air. Taken together, the results show that volatiles from clean plants suppress conidiation, whereas herbivore-induced plant volatiles promote conidiation of N. tanajoae. These opposing effects suggest that the entomopathogenic fungus tunes the release of spores to herbivore-induced plant signals indicating the presence of hosts. © 2005 Springer Science + Business Media, Inc.},
	number = {5},
	journal = {Journal of Chemical Ecology},
	author = {Hountondji, F.C.C. and Sabelis, M.W. and Hanna, R. and Janssen, A.},
	year = {2005},
	keywords = {Herbivore-induced plant volatiles, herbivore-induced plant volatiles, GC-MS, mite, fungal pathogen, Cassava, Fungal pathogen, Conidiation, Mite, Mononychellus tanajoa, Neozygites tanajoae, conidiation},
	pages = {1003--1021},
	file = {Hountondji_2005_Herbivore-induced Plant Volatiles.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\J29CIWU5\\Hountondji_2005_Herbivore-induced Plant Volatiles.pdf:application/pdf},
}

