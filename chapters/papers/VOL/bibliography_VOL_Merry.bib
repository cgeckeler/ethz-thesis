@incollection{blande_chapter_2017,
	series = {How {Plants} {Communicate} with their {Biotic} {Environment}},
	title = {Chapter {Eleven} - {Plant} {Communication} {With} {Herbivores}},
	volume = {82},
	url = {https://www.sciencedirect.com/science/article/pii/S0065229616300908},
	abstract = {Plants and herbivores both release volatile organic compounds that have important roles in mediating important biological functions related to defence and reproduction. Plants emit complex blends of chemicals that are involved in multitrophic interactions, coordination of systemic defence responses and pollination, whereas herbivorous insects release pheromones that play important roles in attracting mates, instigating defence responses and initiating aggregation. Interactions between plants and herbivores have been subject to a wealth of studies and knowledge on their biology, biochemistry, ecology and evolution is constantly expanding. In this chapter the idea of communication between plants and herbivores will be explored. Communication between organisms of consecutive trophic levels is somewhat controversial due to unidirectional reliance and competition precluding some of the requirements of a conventional communication process, but there are growing examples of where chemically mediated interactions between plants and herbivores can be viewed as eavesdropping by a signal recipient, or even as true communication where both chemical emitter and receiver gain a benefit from communication. Examples of herbivores responding to plant-emitted cues and plants responding to herbivore-emitted cues are both explored, and suggestions for future directions in this field are provided.},
	language = {en},
	urldate = {2022-05-21},
	booktitle = {Advances in {Botanical} {Research}},
	publisher = {Academic Press},
	author = {Blande, J. D.},
	editor = {Becard, Guillaume},
	month = jan,
	year = {2017},
	doi = {10.1016/bs.abr.2016.09.004},
	keywords = {Induction, Plant, Volatile organic compounds, Parasitoid, Communication, Herbivore, Predator, Eavesdropping},
	pages = {281--304},
	file = {Blande - Plant Communication With Herbivores.pdf:C\:\\Users\\mschuman\\Zotero\\storage\\HS2TEIBQ\\Blande - Plant Communication With Herbivores.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\mschuman\\Zotero\\storage\\LG5YVPN2\\S0065229616300908.html:text/html},
