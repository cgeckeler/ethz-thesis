@article{ochieng_antennal_1995,
	title = {Antennal lobe projection patterns of olfactory receptor neurons involved in sex pheromone detection in {Spodoptera} littoralis ({Lepidoptera}: {Noctuidae})},
	volume = {27},
	issn = {0040-8166},
	url = {https://www.sciencedirect.com/science/article/pii/S0040816695800247},
	doi = {10.1016/S0040-8166(95)80024-7},
	abstract = {Pheromone-specific receptor neurons in male and female cotton leafworms, Spodoptera littoralis (Boisd.) (Lepidoptera: Noctuidae) were typed physiologically and traced into the antennal lobe using cobalt lysine as a marker. In male S. littoralis, the macroglomerular complex (MGC), which is responsible for the initial integration of information concerning sex pheromone components, contains three glomerular compartments as revealed in a morphological study. No such specialized structure was seen in the female. In the male, olfactory receptor neurons that responded selectively to stimulation with the major sex pheromone component, (Z)9, (E)11-tetradecadienyl acetate (Z9E11-14:OAc), had arborizations that were restricted to a large glomerulus of the MGC (designated a), situated near the antennal nerve entrance into the antennal lobe. Neurons that were stimulated by (Z)9,(E)12-tetradecadienyl acetate (Z9E12-14:OAc), a second pheromone component, had arborizations in a lateral, smaller glomerulus of the MGC (designated c), while receptor neurons specifically tuned to a behavioural antagonist, (Z)-9-tetradecenol (Z9-14:OH), projected to a medial glomerulus of the MGC (b). In the female, receptor neurons tuned to the major pheromone component projected to a glomerulus situated at the entrance of the antennal nerve. This glomerulus did, however, not have the size or the structure of the male MGC. A second neuron housed in the same sensillum projected its axon to an ordinary glomerulus situated medially in the antennal lobe.},
	number = {2},
	journal = {Tissue and Cell},
	author = {Ochieng, Samuel A. and Anderson, Peter and Hansson, Bill S.},
	month = apr,
	year = {1995},
	keywords = {Sex pheromone, insect, moth, antennal lobe, macroglomerular complex, sensillum, cobalt, receptor neuron},
	pages = {221--232},
}

@incollection{barbosa_chapter_1998-1,
	address = {San Diego},
	title = {Chapter 5 - {Influence} of plants on invertebrate predators: {Implications} to conservation biological control},
	isbn = {978-0-12-078147-8},
	url = {https://www.sciencedirect.com/science/article/pii/B9780120781478500517},
	abstract = {Publisher Summary
The chapter analyzes that invertebrate predators, which are abundant in number, can directly affect the population of plant-feeding insects. As the invertebrate predators feed on plant-eating insects, they help in reducing the population of phytophagous arthropods. In the agroecosystem, predator complex helps in reducing the damage caused by insect pests. The chapter also states that the effectiveness of the predators on the phytophagous insects generally depends on certain factors. The factors are generally biotic in nature. Plants can influence the survival and the development of the invertebrate predators. Plants can also directly or indirectly influence the interaction between the invertebrate predators and their prey. Therefore, one can say that the success of the conservation biological control to a great extent depends on the direct or indirect interactions between the predators and their prey. It is important to take necessary steps to conserve predators to have a balanced agroecosystem. The chapter also discusses that to have effective tactics for conservation biological control, one needs to have a clear understanding of how plant affects predators of the phytophagous pests.},
	booktitle = {Conservation {Biological} {Control}},
